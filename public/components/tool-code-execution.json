{
  "id": "tool-code-execution",
  "type": "tool",
  "name": "Code Execution",
  "version": "1.0.0",
  "provider": "e2b",
  "config": {
    "endpoint": "https://api.e2b.dev/v1/sandboxes",
    "auth_type": "bearer",
    "method": "POST",
    "parameters": {
      "required": ["code", "language"],
      "optional": ["timeout", "packages"]
    }
  },
  "interface": {
    "inputs": ["code", "language"],
    "outputs": ["stdout", "stderr", "execution_time"],
    "capabilities": ["python_execution", "sandboxed_environment"]
  },
  "resources": {
    "rate_limits": {
      "requests_per_minute": 30,
      "execution_timeout": 60
    }
  },
  "metadata": {
    "author": "AgentEX",
    "description": "Execute Python code in a secure sandbox",
    "tags": ["code", "execution", "python"],
    "icon": "⚙️",
    "color": "#3776AB"
  }
}
